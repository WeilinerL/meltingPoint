@import "../common/index.wxss";
.van-overlay {
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background-color: rgba(241, 241, 241, 0.7);
    background-color: var(--overlay-background-color, rgba(241, 241, 241, 0.7));
    -webkit-backdrop-filter: blur(4rpx);
    backdrop-filter: blur(4rpx);
}
